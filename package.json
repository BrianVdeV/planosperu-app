{
  "name": "planos-peru",
  "version": "1.0.22",
  "description": "Planos Peru",
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/kimberly31-HC/planosperu-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kimberly31-HC/planosperu-app.git"
  },
  "license": "MIT",
  "author": "The CoreUI Team (https://github.com/orgs/coreui/people)",
  "scripts": {
    "build": "vite build",
    "lint": "eslint",
    "serve": "vite preview",
    "start": "vite",
    "react": "react-scripts start",
    "py": "cd backend && python app.py",
    "py-build": "cd backend && python -m PyInstaller --onefile --add-data \"docs;docs\" app.py",
    "electron:dev": "npm-run-all --parallel flask electron",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel flask react",
    "pack": "electron-builder --dir",
    "electron": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@coreui/chartjs": "^4.1.0",
    "@coreui/coreui": "^5.3.1",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-react": "^2.3.0",
    "@coreui/react": "^5.5.0",
    "@coreui/react-chartjs": "^3.0.0",
    "@coreui/utils": "^2.0.2",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.8.4",
    "chart.js": "^4.4.7",
    "classnames": "^2.5.1",
    "core-js": "^3.40.0",
    "cors": "^2.8.5",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "mysql2": "^3.14.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.5",
    "react-select": "^5.10.1",
    "redux": "5.0.1",
    "simplebar-react": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.2",
    "prettier": "3.5.1",
    "sass": "^1.85.0",
    "vite": "^6.1.0"
  },
  "main": "main.js",
  "build": {
    "appId": "com.planosperu.electron",
    "productName": "Planos Per√∫",
    "icon": "build/favicon.ico",
    "files": [
      "build/**/*",
      "main.js",
      "package.json",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "BrianVdev",
        "repo": "planosperu-app",
        "releaseType": "release"
      }
    ],
    "extraResources": [
      {
        "from": "./backend/dist/app.exe",
        "to": "dist/app.exe"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
